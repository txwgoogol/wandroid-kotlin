<?xml version="1.0" encoding="utf-8"?>
<com.ansen.shape.AnsenLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/wrap"
    android:gravity="center"
    android:orientation="vertical"
    android:padding="10pt"
    app:corners_radius="10pt"
    app:solid_color="@android:color/white">

    <ProgressBar
        style="?android:attr/progressBarStyle"
        android:layout_width="24pt"
        android:layout_height="24pt" />

    <TextView
        android:id="@+id/pro_message"
        style="@style/font_12"
        android:layout_marginTop="10pt"
        android:text="正在加载中..." />

</com.ansen.shape.AnsenLinearLayout>